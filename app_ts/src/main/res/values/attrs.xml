<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="indexBarTextSize" format="dimension" />
    <declare-styleable name="IndexBar">
        <attr name="indexBarTextSize" />
        <attr name="indexBarPressBackground" format="color" />
    </declare-styleable>

    <declare-styleable name="searchedit">
        <attr name="imagewidth" format="dimension" />
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
    </declare-styleable>
    <declare-styleable name="RoundAngleImageView">
        <attr name="roundWidth" format="dimension" />
        <attr name="roundHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="TagFlowLayout">
        <attr name="max_select" format="integer"></attr>
        <attr name="tag_gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>
</resources>